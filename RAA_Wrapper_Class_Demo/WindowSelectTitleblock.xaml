<Window x:Class="RAA_Wrapper_Class_Demo.WindowSelectTitleblock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RAA_Wrapper_Class_Demo"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="400">
    <Grid>
        <StackPanel Orientation="Vertical" VerticalAlignment="Center">
            <Label Content="Select a title block type:" HorizontalAlignment="Center"/>
            <ComboBox x:Name="cmbSelectTblockType" Height="30" DisplayMemberPath="FamilyAndType" Margin="20,0,20,0"/>
        </StackPanel>
    </Grid>
</Window>
